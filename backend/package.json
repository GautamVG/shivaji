{
	"name": "node_app_template",
	"version": "1.0.0",
	"description": "",
	"author": "Delta Shadow",
	"scripts": {
		"start": "node dist/index.js",
		"build": "tsc",
		"dev": "nodemon --config nodemon.json src/index.ts",
		"clean": "rimraf build logs",
		"typeorm": "typeorm-ts-node-commonjs",
		"migration:gen": "npm run typeorm migration:generate src/db/migrations/init -- -p -d src/db/data_source.ts",
		"migration:run": "npm run typeorm migration:run -- -d src/db/data_source.ts"
	},
	"devDependencies": {
		"@types/node": "^20.2.4",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.8",
		"rimraf": "^5.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"class-validator": "^0.14.0",
		"dotenv": "^16.0.3",
		"mysql2": "^3.4.3",
		"reflect-metadata": "^0.1.13",
		"socket.io": "^4.7.1",
		"typeorm": "^0.3.17",
		"winston": "^3.8.2"
	}
}
